
QT += core gui
QT += serialport
QT += multimedia
QT += multimediawidgets

greaterThan(QT_MAJOR_VERSION, 4): QT += widgets

INCLUDEPATH += ../
INCLUDEPATH += Source/

CONFIG += force_debug_info

